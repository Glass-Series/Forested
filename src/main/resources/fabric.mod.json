{
  "schemaVersion": 1,
  "id": "forested",
  "version": "${version}",

  "name": "Forested",
  "description": "A simple backport of forestry's wood and tree types to b1.7.3 StationAPI. Includes an API for adding more trees to the game without clashing with vanilla generation.",
  "authors": [
    "calmilamsy"
  ],
  "contact": {
    "homepage": "https://glass-launcher.net/",
    "sources": "https://github.com/calmilamsy/Forested"
  },

  "license": "LGPLV3",
  "icon": "assets/forested/icon.png",

  "environment": "*",
  "entrypoints": {
    "stationapi:event_bus": [
      "net.glasslauncher.mods.forested.Forested",
      "net.glasslauncher.mods.forested.eventlisteners.init.BlockInit",
      "net.glasslauncher.mods.forested.eventlisteners.init.WoodTextureListener",
      "net.glasslauncher.mods.forested.eventlisteners.init.LeavesTextureListener",
      "net.glasslauncher.mods.forested.eventlisteners.init.TreeGenerationInit",
      "net.glasslauncher.mods.forested.eventlisteners.init.GenericRecipes",
      "net.glasslauncher.mods.forested.eventlisteners.init.SaplingInit"
    ],
    "stationapi:event_bus_client": [
    ],
    "stationapi:event_bus_server": [
    ],
    "gcapi3": [
      "net.glasslauncher.mods.forested.ForestedConfig"
    ]
  },
  "mixins": [
    "forested.mixins.json"
  ],

  "depends": {
    "minecraft": "1.0.0-beta.7.3"
  }
}
